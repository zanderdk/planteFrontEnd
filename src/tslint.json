{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"

        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "interface-over-type-literal": false,
        "max-line-length": false
    }
}
